<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="11"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.bowon.graduationworkdebug.MainMixedView.MixedViewActivity">

    <Button
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:id="@+id/button_for_changeviewtype_map"
        android:background="@drawable/selector_goto_camera_land"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:id="@+id/btn_for_fillter_camera"
        android:background="@drawable/selector_btn_fillter_land"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="75dp"
        android:layout_height="53dp"
        android:id="@+id/btn_for_write_camera"
        android:background="@drawable/selector_goto_writepage_land"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

    <com.example.bowon.graduationworkdebug.AutoFitTextureView
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:id="@+id/cameraTexturePreicew"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />


</RelativeLayout>
